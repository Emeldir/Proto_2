
#include <MCP3008.h>

#define CS_PIN 12
#define CLOCK_PIN 9
#define MOSI_PIN 11
#define MISO_PIN 10

// gjør dette * 8

MCP3008 adc0(CLOCK_PIN, MOSI_PIN, MISO_PIN, CS_PIN);

// gjør dette * 8

void setup () {

fotoresistor_number=50
int value_list[]={}
adc_list[] ={adc1, adc2, adc3, adc4, adc5, adc6, adc7, adc8}
int counter=0

}

void loop () {
for (i in range(8)) {
for j in range(8)) {
value_list[counter]=adc_list[i].readADC[j];
counter+=1
}
}
serial.println(value_list[];
}



